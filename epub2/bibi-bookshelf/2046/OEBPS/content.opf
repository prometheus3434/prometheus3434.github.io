    <dc:title>Abi: kabadayılar, mafya ve derin devlet : inceleme</dc:title>
    <dc:creator xmlns:ns0="http://www.idpf.org/2007/opf" ns0:role="aut" ns0:file-as="Yurdakul, Doğan">Doğan Yurdakul</dc:creator>
