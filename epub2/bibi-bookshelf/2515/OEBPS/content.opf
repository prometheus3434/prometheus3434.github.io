    <dc:title>Bin Dokuz Yüz Seksen Dört</dc:title>
    <dc:creator opf:role="aut" opf:file-as="Orwell, George">George Orwell</dc:creator>
    <dc:identifier id="BookId" opf:scheme="UUID">urn:uuid:521b9a87-6eed-4668-ac0a-e3f35e6aec47</dc:identifier>
