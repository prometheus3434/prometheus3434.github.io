    <dc:title>Beş sevgi dili</dc:title>
    <dc:creator opf:role="aut" opf:file-as="Chapman, Gary D.">Gary D. Chapman</dc:creator>
    <dc:identifier id="BookId" opf:scheme="UUID">urn:uuid:baf0705b-99fb-407a-9986-71509b839bcb</dc:identifier>
