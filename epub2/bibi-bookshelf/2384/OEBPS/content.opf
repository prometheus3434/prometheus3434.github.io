    <dc:title>Barkvik Cinayetleri</dc:title>
    <dc:creator opf:role="aut" opf:file-as="Ambjörnsen, Ingvar">Ingvar Ambjörnsen</dc:creator>
    <dc:identifier id="uuid">{A2789BEA-B002-421A-AB73-F53CF2CA9B0A}</dc:identifier>
