    <dc:title>Dink cinayeti ve istihbarat yalanları</dc:title>
    <dc:creator opf:role="aut" opf:file-as="Şener, Nedim">Nedim Şener</dc:creator>
    <dc:publisher>Güncel Yayıncılık</dc:publisher>
