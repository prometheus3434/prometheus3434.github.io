    <dc:title>Dünyadaki en büyük satıcı</dc:title>
    <dc:creator opf:role="aut" opf:file-as="Mandino, Og">Og Mandino</dc:creator>
    <dc:identifier id="BookId" opf:scheme="UUID">urn:uuid:426d3d75-c696-4526-9fe4-6b91a4584ea9</dc:identifier>
