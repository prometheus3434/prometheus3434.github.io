    <dc:title>Dünya bir kılavuz</dc:title>
    <dc:creator opf:role="aut" opf:file-as="Therborn, Göran &amp; Kılıç, Ebru">Göran Therborn</dc:creator>
    <dc:creator opf:role="aut">Ebru Kılıç</dc:creator>
