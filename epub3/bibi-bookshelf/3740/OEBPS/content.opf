    <dc:title>Kabala Yahudi Kadim Mistik Öğretisi</dc:title>
    <dc:creator opf:role="aut" opf:file-as="Ülkü, A. Ekrem">A. Ekrem Ülkü</dc:creator>
    <dc:identifier id="BookId">awp2012-01-19T21:45:51Z</dc:identifier>
