    <dc:title>Kitaplardan Korkan Çocuk</dc:title>
    <dc:creator opf:role="aut" opf:file-as="Tamaro, Susanna">Susanna Tamaro</dc:creator>
    <dc:publisher>Can Yayınları</dc:publisher>
