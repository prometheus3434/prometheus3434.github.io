    <dc:title>Kız kardeşim için</dc:title>
    <dc:creator opf:role="aut" opf:file-as="Picoult, Jodi &amp; Göktaş, Serkan">Jodi Picoult</dc:creator>
    <dc:creator opf:role="aut">Serkan Göktaş</dc:creator>
