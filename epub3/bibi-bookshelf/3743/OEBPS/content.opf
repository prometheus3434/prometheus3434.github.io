    <dc:title>KAÇ KİLOYA PROGRAMLISIN: Bilinçaltı Diyeti</dc:title>
    <dc:creator xmlns:ns0="http://www.idpf.org/2007/opf" ns0:role="aut" ns0:file-as="Gün, Nil">Nil Gün</dc:creator>
    <dc:date>2011-02-19T22:00:00+00:00</dc:date>
