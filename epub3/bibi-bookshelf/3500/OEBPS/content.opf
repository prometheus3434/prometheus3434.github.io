    <dc:title>Hercai 2: Meftun</dc:title>
    <dc:creator opf:role="aut" opf:file-as="Koç, Sümeyye">Sümeyye Koç</dc:creator>
    <dc:identifier id="uuid">{69773E4E-BF7B-4207-AF36-F50F016BEB71}</dc:identifier>
