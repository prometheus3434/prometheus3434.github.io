    <dc:title>Yolpalas Cinayeti: Selim İleri'nin Sonsözüyle</dc:title>
    <dc:creator xmlns:ns0="http://www.idpf.org/2007/opf" ns0:role="aut" ns0:file-as="Adıvar, Halide Edib">Halide Edib Adıvar</dc:creator>
