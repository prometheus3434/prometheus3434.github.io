    <dc:title>Wool</dc:title>
    <dc:creator opf:role="aut" opf:file-as="Howey, Hugh">Hugh Howey</dc:creator>
    <dc:identifier id="BookId" opf:scheme="UUID">urn:uuid:1e771471-d563-48fe-a9fd-c06ce23e87fd</dc:identifier>
