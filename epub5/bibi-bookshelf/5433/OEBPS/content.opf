    <dc:title>Yağmur kral</dc:title>
    <dc:creator opf:role="aut" opf:file-as="Bellow, Saul &amp; Yener, Osman">Saul Bellow</dc:creator>
    <dc:creator opf:role="aut">Osman Yener</dc:creator>
    <dc:identifier id="uuid">{7FFC131C-5731-4CD3-9B68-E69AC1350D0E}</dc:identifier>
