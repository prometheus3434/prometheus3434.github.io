  <metadata>
    <dc:title>Yabancılaşma: Marx’ın Kapitalist Toplumdaki İnsan Anlayışı</dc:title>
    <dc:creator xmlns:ns0="http://www.idpf.org/2007/opf" ns0:role="aut" ns0:file-as="Ollman, Bertell">Bertell Ollman</dc:creator>
    <dc:publisher>Yordam Kitap</dc:publisher>
