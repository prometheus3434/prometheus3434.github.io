    <dc:title>Yaşlı Ormanın Gizemi</dc:title>
    <dc:creator opf:role="aut" opf:file-as="Buzzati, Dino">Dino Buzzati</dc:creator>
    <dc:identifier id="BookId">AWP-4C066ADF-CAEF-4FB3-BC64-724963E2C17F</dc:identifier>
