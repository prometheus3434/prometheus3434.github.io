    <dc:title>Tanrı ve yeni fizik</dc:title>
    <dc:creator opf:role="aut" opf:file-as="Davies, Paul">Paul Davies</dc:creator>
    <dc:date>2014-06-15T12:40:07+00:00</dc:date>
