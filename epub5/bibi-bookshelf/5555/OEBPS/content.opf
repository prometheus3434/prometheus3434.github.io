    <dc:title>Yenilmez</dc:title>
    <dc:creator opf:role="aut" opf:file-as="Lem, Stanislaw">Stanislaw Lem</dc:creator>
    <dc:identifier id="uuid_id" opf:scheme="uuid">8350ff2e-ba13-4e37-9799-ed32117fe17c</dc:identifier>
