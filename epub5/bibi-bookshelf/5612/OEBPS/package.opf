    <dc:title>Yüreğimin Sesini Dinle</dc:title>
    <dc:creator xmlns:ns0="http://www.idpf.org/2007/opf" ns0:role="aut" ns0:file-as="Tamaro, Susanna">Susanna Tamaro</dc:creator>
