    <dc:title>Kusma kulübü</dc:title>
    <dc:creator opf:role="aut" opf:file-as="Eroğlu, Mehmet">Mehmet Eroğlu</dc:creator>
    <dc:identifier id="uuid">{4CC2BC79-8BE7-4DB3-8146-1CFF680B98BF}</dc:identifier>
