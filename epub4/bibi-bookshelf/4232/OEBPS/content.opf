    <dc:title>Matmazel Noraliya'nın Koltuğu</dc:title>
    <dc:creator opf:role="aut" opf:file-as="Safa, Peyami">Peyami Safa</dc:creator>
    <dc:identifier id="BookId" opf:scheme="UUID">urn:uuid:16a8a5b6-b264-4053-a1ed-85b4d8bebed8</dc:identifier>
