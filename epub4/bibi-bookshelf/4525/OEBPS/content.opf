    <dc:title>Otostopçunun galaksi rehberi</dc:title>
    <dc:creator opf:role="aut" opf:file-as="Adams, Douglas &amp; Alt, Nil">Douglas Adams</dc:creator>
    <dc:creator opf:role="aut">Nil Alt</dc:creator>
