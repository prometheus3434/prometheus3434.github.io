    <dc:title>Kumlarin-Kadini</dc:title>
    <dc:creator opf:role="aut" opf:file-as="Abe, Kobo">Kobo Abe</dc:creator>
    <dc:date>0101-01-01T00:00:00+00:00</dc:date>
