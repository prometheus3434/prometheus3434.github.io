    <dc:title>Küreselleşme ulus-devlet: azınlıklar ve dil</dc:title>
    <dc:creator opf:role="aut" opf:file-as="Çoban, Savaş">Savaş Çoban</dc:creator>
    <dc:identifier id="uuid_id" opf:scheme="uuid">3e1e668b-a85a-4000-92cc-f57cdc18f3c6</dc:identifier>
