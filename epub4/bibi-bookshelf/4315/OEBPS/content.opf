    <dc:title>Moira üçlemesi</dc:title>
    <dc:creator opf:role="aut" opf:file-as="Loevenbruck, Henri &amp; Gökteke, Elif">Henri Loevenbruck</dc:creator>
    <dc:creator opf:role="aut">Elif Gökteke</dc:creator>
