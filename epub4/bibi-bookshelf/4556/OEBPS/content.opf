    <dc:title>Ölüm denizaltıları U-boat'lar</dc:title>
    <dc:creator opf:role="aut" opf:file-as="Mason, David">David Mason</dc:creator>
    <dc:identifier id="uuid_id" opf:scheme="uuid">598cc36b-6c4b-4f7d-9fd3-bde0352b7dbc</dc:identifier>
