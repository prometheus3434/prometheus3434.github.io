    <dc:title>Narcissus’un Zencisi</dc:title>
    <dc:creator xmlns:ns0="http://www.idpf.org/2007/opf" ns0:role="aut" ns0:file-as="Conrad, Joseph">Joseph Conrad</dc:creator>
