    <dc:title>Piyasaları Okumak</dc:title>
    <dc:creator opf:role="aut" opf:file-as="Özerol, Hakan">Hakan Özerol</dc:creator>
    <dc:identifier id="uuid_id" opf:scheme="uuid">1617a383-333b-4211-b38f-5ff673237803</dc:identifier>
