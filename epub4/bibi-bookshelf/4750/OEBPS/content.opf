    <dc:title>Rüzgar çalgısı</dc:title>
    <dc:creator opf:role="aut" opf:file-as="Nicholson, William">William Nicholson</dc:creator>
    <dc:identifier id="uuid">{DBCBBE41-ABB6-4D24-9DF0-2286A6C6963E}</dc:identifier>
