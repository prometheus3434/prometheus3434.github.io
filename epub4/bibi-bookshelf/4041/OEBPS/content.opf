    <dc:title>Korsan gemisi</dc:title>
    <dc:creator opf:role="aut" opf:file-as="Cussler, Clive &amp; Gürsel, Mehmet">Clive Cussler</dc:creator>
    <dc:creator opf:role="aut">Mehmet Gürsel</dc:creator>
