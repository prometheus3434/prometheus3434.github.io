    <dc:title>Sade'ı yakmalı mı?</dc:title>
    <dc:creator opf:role="aut" opf:file-as="Beauvoir, Simone de &amp; Süraya, Cemal">Simone de Beauvoir</dc:creator>
    <dc:creator opf:role="aut">Cemal Süraya</dc:creator>
