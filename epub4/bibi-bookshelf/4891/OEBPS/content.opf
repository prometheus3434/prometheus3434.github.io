    <dc:title>Sıkı kontrol edilen trenler</dc:title>
    <dc:creator opf:role="aut" opf:file-as="Hrabal, Bohumil">Bohumil Hrabal</dc:creator>
    <dc:identifier id="EPB-UUID" opf:scheme="uuid">xxxx-xxxxx-xxxxx-xxxxxxxxx</dc:identifier>
