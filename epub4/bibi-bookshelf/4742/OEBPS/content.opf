    <dc:title>Ruhi Mücerret</dc:title>
    <dc:creator opf:role="aut" opf:file-as="Menteş, Murat">Murat Menteş</dc:creator>
    <dc:identifier id="uuid">{03FCF6D0-3B49-4F2C-8DD6-9DE2F19CDEE8}</dc:identifier>
